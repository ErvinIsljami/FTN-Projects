<Window x:Class="PZ2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:gmf="clr-namespace:GMap.NET.WindowsForms;assembly=GMap.NET.WindowsForms"
        xmlns:gm="clr-namespace:GMap.NET;assembly=GMap.NET.Core"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        xmlns:local="clr-namespace:PZ2"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <WindowsFormsHost Margin="0,35,0,0">
            <gmf:GMapControl x:Name="gmap" Load="Gmap_Load"></gmf:GMapControl>
        </WindowsFormsHost>
        <Button Content="Load Markers" FontSize="22" Foreground="White" Background="Green" VerticalAlignment="Top" Margin="0,0,0,0" Click="LoadMarkers_Click" Height="35"/>
    </Grid>
</Window>
