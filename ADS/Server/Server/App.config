<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

  <system.serviceModel>
    <services>
      
      
      <service name="Server.Banka">
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:15000/Banka" />
          </baseAddresses>
        </host>
        <!-- Service Endpoints -->
        <endpoint address="net.tcp://localhost:15000/Banka" binding="netTcpBinding"
       contract="Common.IBanka" />
      </service>


      <service name="Server.BezbednosniMehanizmi">
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:15001/Autentifikacija" />
          </baseAddresses>
        </host>
        <!-- Service Endpoints -->
        <endpoint address="net.tcp://localhost:15001/Autentifikacija" binding="netTcpBinding"
       contract="Common.IBezbednosniMehanizmi" />
      </service>
      
      
    </services>
  </system.serviceModel>
  
  
</configuration>