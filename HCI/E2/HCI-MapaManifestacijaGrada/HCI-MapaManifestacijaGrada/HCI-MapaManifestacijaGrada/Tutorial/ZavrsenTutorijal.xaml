<Window x:Class="HCI_MapaManifestacijaGrada.Tutorial.ZavrsenTutorijal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HCI_MapaManifestacijaGrada.Tutorial"
        mc:Ignorable="d"
         Title="Zavrsen Tutorijal" Height="200" Width="300"  WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <DockPanel  Background="LightBlue">
            <Grid DockPanel.Dock="Bottom" Height="40" VerticalAlignment="Bottom" Background="CadetBlue">
                <Button Content="Potvrdi" Height="23" HorizontalAlignment="Left" Margin="188,11,0,0" Name="UpozorenjePotvrdi" VerticalAlignment="Top" Width="75" Click="UpozorenjePotvrdi_Click" />
            </Grid>
            <Grid DockPanel.Dock="Top">
                <Label Content="Uspesno ste zavrsili tutorijal!" FontFamily="Helvetica" FontSize="13" Height="28" Name="label1" Margin="0,30,0,63" />
                <Label Content="Mozete poceti sa dodavanjem tipova, etiketa" Name="label2" FontFamily="Helvetica" FontSize="13" Margin="0,53,0,38" />
                <Label Content="i manifestacija!" FontFamily="Helvetica" FontSize="13" Name="label11" Margin="0,73,0,22" />
            </Grid>
        </DockPanel>
    </Grid>
</Window>
